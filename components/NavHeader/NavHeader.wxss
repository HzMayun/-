/* components/NavHeader/NavHeader.wxss */
.header-title {
  color: #666;
  font-size: 28rpx;
}

.header-content {
  display: flex;
  justify-content: space-between;
}

.header-content text:nth-child(1) {
  font-size: 30rpx;
}

.header-content text:nth-child(2) {
  font-size: 20rpx;
  border: 1rpx solid;
  padding: 10rpx 20rpx;
  border-radius: 15rpx;
}